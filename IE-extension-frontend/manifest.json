{
  "name": "IE-encryption",
  "description": "JPEG-tolerant encryption",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "content_scripts": [
    {
      "js": ["content-script.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "icons": {
    "16": "hello_extensions.png",
    "32": "hello_extensions.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
